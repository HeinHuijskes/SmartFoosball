#include <WiFi.h>
#include <HTTPClient.h>


int sensorPin = 2; //define analog pin 2
int value = 0;
#define LED_PIN 5





void setup() {
  // init_wifi()
	Serial.begin(9600);
  pinMode(LED_PIN, OUTPUT);
  digitalWrite(LED_PIN, HIGH);
  // analogWrite(LED_PIN, 20);

}

void loop() {
	value = analogRead(sensorPin);
	Serial.println(value, DEC); // light intensity
  // value = 0;
	delay(100);
  // goal if value is higher than... for a length of...
  // determine the team
  // if goal, send call to endpoint laptop -> we need wifi on the esp
  // later:update LEDs
}


